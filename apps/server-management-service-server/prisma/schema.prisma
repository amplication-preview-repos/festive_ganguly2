datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ServerCredential {
  createdAt  DateTime @default(now())
  hostname   String?
  id         String   @id @default(cuid())
  name       String?
  port       Int?
  privateKey String?
  updatedAt  DateTime @updatedAt
  username   String?
}
